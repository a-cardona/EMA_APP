# Convert all "Checked"/"Unchecked" columns to binary (1/0) no non binary allowed
renamed_dfs <- lapply(renamed_dfs, function(df) {
df[] <- lapply(df, function(col) {
if (is.character(col) && all(na.omit(col) %in% c("Checked", "Unchecked"))) {
return(ifelse(col == "Checked", 1, 0))
} else {
return(col)
}
})
return(df)
})
